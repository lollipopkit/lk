class speed {
    'times': 5,
}

fn speed:test(func) {
    times := 0
    t1 := os.time()
    
    while times < self.times {
        func()
        times++
    }

    t2 := os.time()
    printf('time: %d ms', (t2 - t1) / 1000)
}

import 'test/fib'

runner := new(speed)
fib30 := fn() => fib(30)
runner:test(fib30)